{
  "extends": ["tslint:latest", "tslint-config-prettier", "tslint-no-unused-expression-chai"],
  "rules": {
    "max-line-length": {
      "options": [140]
    },
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": [true, "log"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "trailing-comma": true,
    "no-unused-expression-chai": true
  }
}
